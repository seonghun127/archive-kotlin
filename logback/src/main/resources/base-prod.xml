    <property name="LOG_PATH" value="./logback/log/"/>
    <include resource="appender.xml" />

    <root level="INFO">
        <appender-ref ref="ASYNC" />
    </root>

    <logger name="com.example.logback.controller" level="TRACE"/>
</included>
